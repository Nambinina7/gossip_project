<div class="text-center">
	<h1>Editer le Gossip</h1>

<%= form_tag url_for(action: 'update'), method: "patch" do %>

  <%= label_tag 'Title' %>
  <%= text_field_tag 'title' %>
  <br /> <br />

  <%= label_tag 'content' %>
  <%= text_area_tag 'content'%>
  <br /> <br />


  <%= submit_tag 'Update Gossip' %>

<% end %>
</div>